{
  "name": "gtl",
  "version": "0.0.0",
  "description": "GTL API",
  "main": "server.js",
  "author": "GTL Team",
  "private": true,
  "dependencies": {
    "@types/express": "4.16.0",
    "@types/mocha": "2.2.41",
    "@types/mongodb": "2.2.8",
    "@types/mongoose": "4.7.19",
    "body-parser": "1.17.2",
    "child-process-promise": "^2.2.1",
    "compression": "^1.7.3",
    "cpy-cli": "1.0.1",
    "del-cli": "1.1.0",
    "express": "4.15.3",
    "express-mongo-sanitize": "^1.3.2",
    "moment": "2.18.1",
    "mongodb": "2.2.33",
    "mongoose": "4.13.2",
    "mongoose-validators": "0.1.0",
    "tcp-port-used": "^1.0.0",
    "typescript": "2.7.2",
    "validator": "8.0.0"
  },
  "engines": {
    "node": "6.11.2"
  },
  "scripts": {
    "test:integration": "nyc mocha --opts integrationTest/mocha.opts",
    "test:unit": "nyc mocha",
    "test": "npm run test:unit",
    "docs": "apidoc -i app -o doc",
    "prebuild": "del-cli dist",
    "build": "node ./node_modules/typescript/bin/tsc && cpy package.json dist --parents",
    "postinstall": "npm run build",
    "clean": "del-cli -f dist dbs doc ext-doc",
    "linux": "npm install && node dist/server.js --local"
  },
  "devDependencies": {
    "@types/chai": "4.1.2",
    "@types/chai-http": "3.0.5",
    "@types/express": "4.16.0",
    "@types/sinon": "4.3.1",
    "apidoc": "0.17.6",
    "chai": "4.1.2",
    "chai-http": "4.0.0",
    "nyc": "13.1.0",
    "mocha": "3.5.0",
    "mocha-bamboo-reporter": "^1.1.0",
    "sinon": "4.4.0",
    "sinon-mongoose": "2.1.1",
    "supertest": "^1.0.1",
    "ts-node": "4.1.0",
    "tslint": "5.10.0",
    "tslint-eslint-rules": "^5.4.0"
  },
  "apidoc": {
    "name": "Belote Poker Meetings API",
    "version": "0.0.0",
    "description": "RESTful API for BPM",
    "title": "BPM API Docs",
    "url": "http://localhost:1337",
    "sampleUrl": "http://localhost:1337/v2"
  },
  "-vs-binding": {
    "BeforeBuild": [
      "install",
      "docs"
    ]
  }
}
